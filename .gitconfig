[user]
    name = Jay Chin
    email = jay.s.chin@gmail.com 

[push]
    default=simple

[url "https://bitbucket.org/"]
        insteadOf = bb:

[url "https://github.com/"]
        insteadOf = gh:

[core]
    editor = vim
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol

[color]
    ui = auto
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[github]
    user = jay-chin 
    token = token
